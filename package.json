{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "create-nif-cif",
  "title": "Create Nif/Cif",
  "description": "",
  "icon": "command-icon.png",
  "author": "AliciaG",
  "license": "MIT",
  "commands": [
